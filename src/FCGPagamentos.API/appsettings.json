{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Postgres": ""
  },
  "AzureStorage": {
    "ConnectionString": ""
  },
  "InternalAuth": {
    "Token": ""
  },
  "ApplicationInsights": {
    "ConnectionString": ""
  },
  "Jaeger": {
    "Host": "localhost",
    "Port": 6831
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [ "FromLogContext" ],
    "WriteTo": [
      { "Name": "Console" }
    ]
  },
  "OpenTelemetry": {
    "EnableConsoleExporter": false,
    "Tracing": {
      "SamplingRatio": 0.01
    }
  }
}
